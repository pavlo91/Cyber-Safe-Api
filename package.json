{
  "name": "cybersafely-api",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "git@github.com:wonderkiln/cybersafely-api.git",
  "author": "Adrian Mateoaea <adrianmteo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --exit-child --quiet --cls -r dotenv/config src/index.ts",
    "dev:cron": "ts-node -r dotenv/config src/crons/index-dev.ts",
    "dev:initial": "rm -rf prisma/migrations && yarn reset && yarn migrate initial",
    "reset": "prisma migrate reset --force",
    "migrate": "prisma migrate dev --name",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate",
    "build": "tsc",
    "start": "prisma migrate deploy && node dist/index.js",
    "staff": "node dist/scripts/staff.js"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.4",
    "@types/node-cron": "^3.0.7",
    "@types/pug": "^2.0.6",
    "dotenv": "^16.0.3",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.290.0",
    "@aws-sdk/s3-request-presigner": "^3.290.0",
    "@fastify/cors": "^8.2.0",
    "@ngneat/falso": "^6.4.0",
    "@pothos/core": "^3.27.1",
    "@pothos/plugin-dataloader": "^3.15.0",
    "@pothos/plugin-scope-auth": "^3.18.0",
    "@pothos/plugin-with-input": "^3.10.0",
    "@prisma/client": "^4.11.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "dataloader": "^2.2.2",
    "fastify": "^4.13.0",
    "file-type": "^16.0.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-yoga": "^3.7.0",
    "jsonwebtoken": "^9.0.0",
    "node-cron": "^3.0.2",
    "postmark": "^3.0.15",
    "pug": "^3.0.2",
    "twitter-api-sdk": "^1.2.1",
    "zod": "^3.20.6"
  }
}
